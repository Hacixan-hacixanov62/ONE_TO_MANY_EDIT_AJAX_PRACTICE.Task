@model HomeVM
@{
    ViewData["Title"] = "Index";
}

<section id="banner">
    <img src="~/assets/images/hero-1-circle.png" alt="" />
    <img src="~/assets/images/hero-1-circle-2.png" alt="" />
    <img src="~/assets/images/hero-1-circle-4.png" alt="" />
    <img src="~/assets/images/hero-sm-1.jpg" alt="" />
    <img src="~/assets/images/hero-1-dot-2.png" alt="" />
    <div class="custom-container">
        <div class="row">
            <div class="col-12 col-md-6">
                <div class="banner-left">
                    <!-- <span>Access 2700+</span> -->
                    <h3>
                        <span>Access 2700+</span><span class="yellow-shape">
                            Online
                            <img src="~/assets/images/yellow-bg.png" alt="yellow-shape" />
                        </span>
                        Tutorial From Top Instructor.
                    </h3>
                    <p>
                        Meet university,and cultural institutions, who'll share their
                        experience.
                    </p>
                    <a href="" class="mt-3">View all course</a>
                </div>
            </div>
            <div class="col-12 col-md-6">
                <div class="banner-right">
                    <img src="~/assets/images/hero-1.jpg" alt="" />
                    <div class="slogan">
                        <span>Tomorrow is our</span>
                        <p>“When I Grow Up” Spirit Day!</p>
                    </div>
                    <img src="~/assets/images/hero-1-dot.png" alt="" />
                    <img src="~/assets/images/hero-1-circle-3.png" alt="" />
                </div>
            </div>
        </div>
    </div>
</section>
<section id="popular-courses" class="mt-5">
    <div class="custom-container">
        <div class="section-head d-flex justify-content-between align-items-center">
            <h2>
                Explore <br />Our
                <span>
                    Popular
                    <img src="~/assets/images/yellow-bg.png" alt="" />
                </span>Courses
            </h2>
            <div class="more-categories">
                <a href="">
                    View all Category
                    <i class="fa-solid fa-arrow-right"></i>
                </a>
            </div>
        </div>
        <div class="row">
         @foreach (var item in Model.Categories.Take(9))
            {
                <div class="col-12 col-md-4 mt-3 mt-md-4">
                    <div class="course course-1 d-flex align-items-center">
                        <img src="~/assets/images/@item.Image" style="height:40px;width:40px" />
                        <div class="course-name">
                            <a href="">@item.Name</a>
                            <span class="d-block">@item.Description</span>
                        </div>
                    </div>
                </div>
            }
        
        </div>
        <div class="row mt-5">
            <div class="col-12 col-md-6 mt-3 mt-md-0">
                <div class="online-course online-course-1 d-flex">
                    <div class="course-name">
                        <span>Free</span>
                        <a href="" class="mt-2">Germany Foundation Document</a>
                        <a href="" class="mt-3">View Courses</a>
                    </div>
                    <img src="~/assets/images/banner-img-1.png" alt="" />
                </div>
            </div>
            <div class="col-12 col-md-6 mt-3 mt-md-0">
                <div class="online-course online-course-2 d-flex">
                    <div class="course-name">
                        <span>Free</span>
                        <a href="" class="mt-2">Online Courses From Eduka University</a>
                        <a href="" class="mt-3">View Courses</a>
                    </div>
                    <img src="~/assets/images/banner-img-2.png" alt="" />
                </div>
            </div>
        </div>
    </div>
</section>
